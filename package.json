{
  "name": "gds-nodejs-boilerplate",
  "description": "A Node.js project boilerplate for production apps",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">6.12.2"
  },
  "xo": {
    "space": true,
    "semicolon": false
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "server": "node ./start.js",
    "start": "npm run compile && npm run server",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "lint-sass": "./node_modules/.bin/sass-lint -v",
    "test": "xo && node ./node_modules/mocha/bin/mocha '!(node_modules)/**/*+(.test|.tests)'.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "body-parser": "1.18.x",
    "client-sessions": "0.8.x",
    "compression": "1.7.x",
    "correlation-id": "^2.0.0",
    "express": "4.16.x",
    "i18n": "0.8.x",
    "lodash": "^4.17.4",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "nunjucks": "^3.0.1",
    "pino": "4.10.x",
    "readdir": "0.0.x",
    "requestretry": "^1.12.0",
    "serve-favicon": "2.4.5",
    "staticify": "0.0.x",
    "throng": "4.0.x",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "8.0.x",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.1",
    "chokidar-cli": "latest",
    "envfile": "^2.1.1",
    "eslint": "^3.19.0",
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.2.0",
    "govuk_template_jinja": "^0.23.0",
    "grunt": "1.0.x",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-watch": "1.0.x",
    "grunt-env": "0.4.x",
    "grunt-mocha-test": "^0.13.2",
    "grunt-nodemon": "0.4.x",
    "grunt-rewrite": "1.0.x",
    "grunt-sass": "2.0.x",
    "grunt-text-replace": "0.4.x",
    "jquery": "^3.2.1",
    "mocha": "4.0.x",
    "nock": "^9.0.13",
    "node-sass": "4.7.x",
    "nunjucksify": "^2.2.0",
    "rfc822-validate": "^1.0.0",
    "sass-lint": "^1.10.2",
    "snyk": "1.54.x",
    "supertest": "^3.0.0",
    "xo": "^0.18.2"
  },
  "snyk": true
}
